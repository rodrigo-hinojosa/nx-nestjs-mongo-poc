{
    "extends": [
        "../../.eslintrc.json"
    ],
    "ignorePatterns": [
        "!**/*"
    ],
    "overrides": [
        {
            "files": [
                "*.ts",
                "*.js"
            ],
            "rules": {
                "quotes": [
                    2,
                    "single",
                    {
                        "avoidEscape": true
                    }
                ]
            }
        }
    ]
}
